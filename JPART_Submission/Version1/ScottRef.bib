@article{martin2001-a,
	title = {Geography and public policy: The case of the missing agenda.},
	volume = {16},
	issn = {0885-4122},
	abstract = {The past two decades have seen a wholesale rethinking and reworking of public policy and have provided geographers with a major opportunity to enter and help shape the policy debated. Yet, disappointingly, the effect of geography on the policy realm has been limited. Increasingly, it seems, other social, political, and environmental scientists, and even media pundits, shape public perception and government policy in areas where geographers could-indeed should-be having much greater influence. In this article, the authors examine the reasons for this state of affairs. The fundamental problem, he contends, is that for several reasons, much contemporary social and economic geographic research renders it of little practical relevance for policy, in some cases of little social relevance at all. The more significant reasons for this lack of relevance to, and influence on, the policy realm include the following: the effect on the subject of the postmodern and cultural "turns"; the consequential emphasis on "sexy" philosophical, linguistic, and theoretical issues rather than on practical social research; the retreat from detailed, rigorous empirical work; the intellectual bias against policy studies; and the lack of political commitment. The article makes a plea for a new "policy turn" in the discipline and concludes with some tentative suggestions as to how geographers might move toward a discipline of public policy.},
	number = {1},
