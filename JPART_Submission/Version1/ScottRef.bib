@article{goodin1982,
	title = {Discounting Discounting},
	volume = {2},
	issn = {{0143814X}},
	url = {http://www.jstor.org/stable/3998211},
	number = {1},
	journal = {Journal of Public Policy},
	author = {Goodin, Robert E.},
	month = feb,
	year = {1982},
	pages = {53--71},
	annote = {Policy analysts typically presume that future payoffs should be discounted relative to present ones, and that this discounting should proceed at the same rate for all goods and all periods. Closer inspection of four arguments for discounting, however, shows these practices to be problematic. Only two of those arguments provide plausible justifications for time-discounting at all; and neither of those justify a constant rate throughout all periods or for all goods. Indeed, there is an important class of 'nontradable' goods which can be discounted only in their own terms. The overall conclusion is that policymakers ought, logically as well as morally, to weight the interests of the future far more heavily than in ordinary discounting procedures.}
},

